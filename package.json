{
    "name": "autoflare",
    "version": "0.4.8",
    "description": "A declarative lightweight library for dynamic error-driven SQL schema synchronization, and automatic management and implementations of various Cloudflare services and features.",
    "keywords": [
        "typescript",
        "sql",
        "database",
        "schema",
        "auto-schema",
        "dynamic-schema",
        "cloudflare-d1",
        "orm",
        "database-management",
        "sql-schema",
        "typescript-database",
        "mail",
        "cloudflare-mail"
    ],
    "scripts": {
        "prepare": "node prepare-publish.js",
        "build": "rimraf dist && tsc",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "author": "Beyondo",
    "license": "MIT",
    "devDependencies": {
        "fs-extra": "^11.2.0",
        "rimraf": "^5.0.5",
        "typescript": "^5.3.3"
    },
    "exports": {
        "./d1": {
            "import": "./dist/d1/index.js",
            "types": "./dist/d1/index.d.ts"
        },
        "./mail": {
            "import": "./dist/mail/index.js",
            "types": "./dist/mail/index.d.ts"
        }
    },
    "type": "commonjs",
    "files": [
        "dist",
        "LICENSE",
        "README.md"
    ]
}
